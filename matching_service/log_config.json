{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "uvicorn_default": {
            "format": "%(asctime)s:UVICORN:%(levelname)s:%(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        },
        "default": {
            "format": "%(asctime)s:%(filename)s:%(funcName)s:%(levelname)s:%(lineno)s:%(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "uvicorn_default": {
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout",
            "formatter": "uvicorn_default"
        },
        "uvicorn_fileHandler": {
            "class": "logging.FileHandler",
            "filename": "app.log",
            "formatter": "uvicorn_default",
            "encoding": "utf-8"
        },
        "default": {
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout",
            "formatter": "default"
        },
        "fileHandler": {
            "class": "logging.FileHandler",
            "filename": "app.log",
            "formatter": "default",
            "encoding": "utf-8"
        }
    },
    "loggers": {
        "uvicorn.error": {
            "handlers": ["uvicorn_default", "uvicorn_fileHandler"],
            "level": "INFO",
            "propagate": false
        },
        "uvicorn.access": {
            "handlers": ["uvicorn_default", "uvicorn_fileHandler"],
            "level": "INFO",
            "propagate": false
        },
        "uvicorn": {
            "handlers": ["uvicorn_default", "uvicorn_fileHandler"],
            "level": "INFO"
        },
        "watchfiles.main": {
            "handlers": ["uvicorn_default", "uvicorn_fileHandler"],
            "level": "WARNING"
        }
    },
    "root": {
        "handlers": ["default", "fileHandler"],
        "level": "INFO"
    }
}
